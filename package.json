{
  "name": "tfw-platform-sceneeditor",
  "version": "1.0.0",
  "description": "Scene editor for the tfw multiplayer game platform",
  "author": "tfw, inc. (https://tfw.dev)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tfwdev/sceneeditor.git"
  },
  "sideEffects": false,
  "scripts": {
    "postinstall": "yarn link --cwd ../platform/lib ; yarn link tfw",
    "start": "webpack-dev-server --config webpack.dev.js",
    "webpack": "webpack --config webpack.prod.js",
    "build": "tsc -b",
    "server": "NODE_PATH=./node_modules node lib/server.js"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    }
  },
  "dependencies": {
    "tfw": "file:../platform/lib"
  },
  "devDependencies": {
    "awesome-typescript-loader": "^5.2.0",
    "lodash": "^4.17.13",
    "ts-jest": "^24.0.0",
    "typescript": "^3.5.1",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.1"
  }
}
